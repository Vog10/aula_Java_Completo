package estrutura_repetitiva;

import java.util.Scanner;

public class Exercicio_for3 {

	public static void main(String[] args) {
		Scanner vogin = new Scanner(System.in);

		int n = vogin.nextInt();
		int soma;
		for (int i = 0; i < n; i++) {

			int a = vogin.nextInt();
			int b = vogin.nextInt();

			if (b == 0) {

				System.out.println("impossivel calcula");
			} else {

				soma = a + b;
			}

		}
		vogin.close();
	}

}
